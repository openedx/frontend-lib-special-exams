{
  "name": "@edx/frontend-lib-special-exams",
  "version": "1.14.0",
  "description": "Special exams lib",
  "main": "dist/index.js",
  "release": {
    "branches": [
      "main"
    ]
  },
  "exports": {
    "import": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-lib-special-exams.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "make build",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-lib-special-exams",
  "bugs": {
    "url": "https://github.com/edx/frontend-lib-special-exams/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "babel-polyfill": "6.26.0",
    "eventemitter3": "^4.0.7"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^1.8.4",
    "@edx/paragon": "^19.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "@reduxjs/toolkit": "^1.5.1"
  },
  "devDependencies": {
    "@edx/frontend-build": "^5.6.11",
    "@edx/frontend-platform": "1.8.4",
    "@edx/paragon": "19.14.1",
    "@testing-library/dom": "7.16.3",
    "@testing-library/jest-dom": "5.10.1",
    "@testing-library/react": "10.3.0",
    "axios-mock-adapter": "1.18.2",
    "codecov": "3.7.2",
    "es-check": "5.1.4",
    "glob": "7.1.6",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "@reduxjs/toolkit": "^1.5.1",
    "reactifex": "1.1.1",
    "react-intl": "2.9.0",
    "semantic-release": "17.2.3",
    "rosie": "2.0.1"
  }
}
